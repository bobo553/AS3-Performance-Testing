<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/halo" minWidth="1024" minHeight="768" creationComplete="application1_creationCompleteHandler(event)">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import com.gskinner.utils.PerformanceTest;
			
			import mx.events.FlexEvent;
			
			import performancetests.ClassInstanceVersusHasOwnProperty;
			

			protected function application1_creationCompleteHandler(event:FlexEvent):void
			{
				var perfTest:PerformanceTest = PerformanceTest.getInstance();
				perfTest.out = out;
				perfTest.testSuite(new ClassInstanceVersusHasOwnProperty());
			}
			
			protected function out(str:*):void {
				//outFld.appendText(String(str)+"\n");
				//outFld.dispatchEvent(new Event(Event.CHANGE));
				trace(str);
			}
		]]>
	</fx:Script>
</s:Application>
